(if
    (matches (window_name) ".*Vimperator$")
    (begin
       (unmaximize)
       (set_workspace 2)
       (maximize)
    )
)

(if 
    (is (application_name) "emacs24")
    (begin
        (unmaximize)
        (set_workspace 3)
        (maximize)
    )
)

(if 
    (matches (window_name) ".*Chromium$")
    (begin
        (unmaximize)
        (set_workspace 6)
        (maximize)
    )
)


